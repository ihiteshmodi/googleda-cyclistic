   "id": "5a2cb4d7",
   "metadata": {},
   "outputs": [],
   "source": [
    "# converting started_at and ended_at columns to date time format\n",
    "customer_data.loc[:, 'started_at'] = pd.Series(pd.to_datetime(customer_data['started_at'], \n",
    "                                    format='%Y/%m/%d %H:%M:%S'), \n",
    "                                    name='started_at', \n",
    "                                    index=customer_data['started_at'].index)\n",
    "customer_data.loc[:, 'ended_at'] = pd.Series(pd.to_datetime(customer_data['ended_at'], \n",
    "                                    format='%Y/%m/%d %H:%M:%S'), \n",
    "                                    name='ended_at', \n",
    "                                    index=customer_data['ended_at'].index)"
   ]
  },
