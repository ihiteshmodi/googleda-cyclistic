   "source": [
    "# creating a date, year, weekday, week and month colums so we can aggregate our data better\n",
    "\n",
    "\n",
    "# Creating a year column\n",
    "customer_data['trips_year'] = customer_data['started_at'].dt.year\n",
    "\n",
    "# Creating a date column\n",
    "customer_data['trips_date'] = customer_data['started_at'].dt.date\n",
    "\n",
    "#Creating a month column\n",
    "customer_data['trips_month'] = customer_data['started_at'].dt.strftime(\"%B\")\n",
    "\n",
    "#Creating a weekday column\n",
    "customer_data['trips_day_of_the_week'] = customer_data['started_at'].dt.strftime(\"%A\")\n",
    "\n",
    "#Creating a week column\n",
    "customer_data['trips_week_of_the_year'] = customer_data['started_at'].dt.weekofyear"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 103,
   "id": "73e4e5c2",
   "metadata": {},
   "outputs": [],
   "source": [
    "# this gave output as 0 days 7 mins 26 seconds aise. we want in seconds only so new code on next line.\n",
    "customer_data['ride_length'] = customer_data['ended_at'] - customer_data['started_at']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 107,
   "id": "b0351104",
   "metadata": {},
   "outputs": [],
   "source": [
    "customer_data['ride_length'] = (customer_data['ended_at'] - customer_data['started_at']).dt.total_seconds()"
   ]
  },
